<%= form_for [:admin, @business_type] do |f| %>
  <div class="form-group">
    <%= render "shared/error_messages", object: @business_type %>
  </div>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit yield: :submit_button, class: "btn btn-primary" %>
  </div>
<% end %>
